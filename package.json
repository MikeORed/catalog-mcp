{
  "name": "catalog-mcp",
  "version": "0.1.0",
  "description": "MCP server for catalog management",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathPatterns=test/unit",
    "test:integration": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathPatterns=test/integration",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "keywords": [
    "mcp",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chokidar": "^5.0.0",
    "csv-parse": "^6.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0"
  }
}
